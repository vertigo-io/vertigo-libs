{
  "name": "@vertigo-io/vertigo-ui-vuejs",
  "version": "3.0.0-RC",
  "scripts": {
    "serve": "vue-cli-service serve --port 3000",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name VertigoUi --filename vertigo-ui src/main.js && cp dist/vertigo-ui.* ../vertigo-ui/src/main/resources/io/vertigo/ui/static/js/",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.7.0",
    "ol": "^5.3.3",
    "quasar": "1.14.3",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "quasar": "1.14.3",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
