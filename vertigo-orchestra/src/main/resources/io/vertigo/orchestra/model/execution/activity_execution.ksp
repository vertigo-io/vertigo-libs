  fkFieldName : "NOD_ID"
    
 	dtDefinitionA : DT_O_ACTIVITY_EXECUTION
	dtDefinitionB : DT_O_NODE
			
	navigabilityA : "false"
	navigabilityB : "true"
	
	multiplicityA:"0..*"
	multiplicityB:"1..1"
	
	labelA : "ExecutionActivity"
	labelB : "Node"
	roleA : "ExecutionActivity"
	roleB : "Node"
			
}

